@prefix : <http://kairos.ai/ont/core#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

# FIBO Imports
@prefix fibo-fnd-aap-agt: <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> .
@prefix fibo-fnd-aap-ppl: <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/> .
@prefix fibo-fnd-org-fm: <https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/> .
@prefix fibo-fnd-org-org: <https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/Organizations/> .
@prefix fibo-fnd-agr-ctr: <https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/> .
@prefix fibo-fnd-law-cor: <https://spec.edmcouncil.org/fibo/ontology/FND/Law/LegalCore/> .
@prefix fibo-fnd-rel-rel: <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/> .
@prefix fibo-fnd-dt-fd: <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/> .
@prefix fibo-fnd-pas-pas: <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/> .

# Ontology Metadata
: a owl:Ontology ;
    dcterms:title "Kairos Core Ontology" ;
    dcterms:description "Core business entities for the Kairos platform - example ontology for prototype" ;
    dcterms:creator "Kairos Ontology Team" ;
    dcterms:created "2026-01-02"^^xsd:date ;
    dcterms:modified "2026-01-03"^^xsd:date ;
    owl:versionInfo "1.0.1" ;
    # FIBO imports using canonical URIs (resolved via catalog-v001.xml)
    owl:imports <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/Agents/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/AgentsAndPeople/People/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/FormalOrganizations/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Organizations/Organizations/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Agreements/Contracts/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Law/LegalCore/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/Relations/Relations/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/DatesAndTimes/FinancialDates/> ,
                <https://spec.edmcouncil.org/fibo/ontology/FND/ProductsAndServices/ProductsAndServices/> .

# Classes

:Customer a owl:Class ;
    rdfs:label "Customer" ;
    rdfs:comment "An individual or organization that purchases products or services from the platform." .

:Order a owl:Class ;
    rdfs:label "Order" ;
    rdfs:comment "A request to purchase products or services, placed by a customer." .

:Product a owl:Class ;
    rdfs:label "Product" ;
    rdfs:comment "A good or service offered for sale on the platform." .

:Service a owl:Class ;
    rdfs:subClassOf fibo-fnd-pas-pas:Service ;
    rdfs:label "Service" ;
    rdfs:comment "A service offered by the Kairos platform, extending FIBO Service concept." .

:ConsultingService a owl:Class ;
    rdfs:subClassOf :Service ;
    rdfs:label "Consulting Service" ;
    rdfs:comment "Professional advisory and consulting services." .

:TechnicalService a owl:Class ;
    rdfs:subClassOf :Service ;
    rdfs:label "Technical Service" ;
    rdfs:comment "Software development, infrastructure, and technical support services." .

:TrainingService a owl:Class ;
    rdfs:subClassOf :Service ;
    rdfs:label "Training Service" ;
    rdfs:comment "Educational and skills development services." .

# Datatype Properties

:customerId a owl:DatatypeProperty ;
    rdfs:label "customer ID" ;
    rdfs:comment "Unique identifier for a customer." ;
    rdfs:domain :Customer ;
    rdfs:range xsd:string .

:email a owl:DatatypeProperty ;
    rdfs:label "email" ;
    rdfs:comment "Email address for customer communication." ;
    rdfs:domain :Customer ;
    rdfs:range xsd:string .

:name a owl:DatatypeProperty ;
    rdfs:label "name" ;
    rdfs:comment "Full name of the customer or product." ;
    rdfs:domain [ a owl:Class ; owl:unionOf (:Customer :Product) ] ;
    rdfs:range xsd:string .

:orderId a owl:DatatypeProperty ;
    rdfs:label "order ID" ;
    rdfs:comment "Unique identifier for an order." ;
    rdfs:domain :Order ;
    rdfs:range xsd:string .

:orderDate a owl:DatatypeProperty ;
    rdfs:label "order date" ;
    rdfs:comment "Date when the order was placed." ;
    rdfs:domain :Order ;
    rdfs:range xsd:date .

:totalAmount a owl:DatatypeProperty ;
    rdfs:label "total amount" ;
    rdfs:comment "Total monetary amount of the order." ;
    rdfs:domain :Order ;
    rdfs:range xsd:decimal .

:productId a owl:DatatypeProperty ;
    rdfs:label "product ID" ;
    rdfs:comment "Unique identifier for a product." ;
    rdfs:domain :Product ;
    rdfs:range xsd:string .

:price a owl:DatatypeProperty ;
    rdfs:label "price" ;
    rdfs:comment "Price of the product." ;
    rdfs:domain :Product ;
    rdfs:range xsd:decimal .

:serviceId a owl:DatatypeProperty ;
    rdfs:label "service ID" ;
    rdfs:comment "Unique identifier for a service." ;
    rdfs:domain :Service ;
    rdfs:range xsd:string .

:serviceName a owl:DatatypeProperty ;
    rdfs:label "service name" ;
    rdfs:comment "Name of the service offering." ;
    rdfs:domain :Service ;
    rdfs:range xsd:string .

:servicePrice a owl:DatatypeProperty ;
    rdfs:label "service price" ;
    rdfs:comment "Base price for the service." ;
    rdfs:domain :Service ;
    rdfs:range xsd:decimal .

:serviceDuration a owl:DatatypeProperty ;
    rdfs:label "service duration" ;
    rdfs:comment "Expected duration to complete the service (ISO 8601 duration format)." ;
    rdfs:domain :Service ;
    rdfs:range xsd:duration .

:deliveryMethod a owl:DatatypeProperty ;
    rdfs:label "delivery method" ;
    rdfs:comment "Method of service delivery (e.g., online, on-site, hybrid)." ;
    rdfs:domain :Service ;
    rdfs:range xsd:string .

# Object Properties

:placedBy a owl:ObjectProperty ;
    rdfs:label "placed by" ;
    rdfs:comment "Indicates which customer placed an order." ;
    rdfs:domain :Order ;
    rdfs:range :Customer .

:hasOrder a owl:ObjectProperty ;
    rdfs:label "has order" ;
    rdfs:comment "Indicates that a customer has placed an order." ;
    rdfs:domain :Customer ;
    rdfs:range :Order ;
    owl:inverseOf :placedBy .

:contains a owl:ObjectProperty ;
    rdfs:label "contains" ;
    rdfs:comment "Indicates that an order contains a product." ;
    rdfs:domain :Order ;
    rdfs:range :Product .

:providedBy a owl:ObjectProperty ;
    rdfs:label "provided by" ;
    rdfs:comment "Indicates which organization provides a service." ;
    rdfs:domain :Service ;
    rdfs:range :Supplier .

:ordersService a owl:ObjectProperty ;
    rdfs:label "orders service" ;
    rdfs:comment "Indicates that an order includes a service." ;
    rdfs:domain :Order ;
    rdfs:range :Service .

:Supplier a owl:Class ;
    rdfs:label "Supplier" ;
    rdfs:comment "An organization that provides products to the platform." .

:supplierId a owl:DatatypeProperty ;
    rdfs:label "supplier ID" ;
    rdfs:comment "Unique identifier for a supplier." ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:supplierName a owl:DatatypeProperty ;
    rdfs:label "supplier name" ;
    rdfs:comment "Name of the supplier organization." ;
    rdfs:domain :Supplier ;
    rdfs:range xsd:string .

:suppliedBy a owl:ObjectProperty ;
    rdfs:label "supplied by" ;
    rdfs:comment "Indicates which supplier provides a product." ;
    rdfs:domain :Product ;
    rdfs:range :Supplier .